# Domain configuration
DOMAIN=evolution.yourdomain.com

# API Authentication
AUTHENTICATION_API_KEY=change-me-to-a-secure-key

# Database Configuration
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI=postgresql://evolution:evolution_password@postgresql:5432/evolution
DATABASE_SAVE_DATA_INSTANCE=true
DATABASE_SAVE_DATA_NEW_MESSAGE=true
DATABASE_SAVE_MESSAGE_UPDATE=true
DATABASE_SAVE_DATA_CONTACTS=true
DATABASE_SAVE_DATA_CHATS=true

# Server Configuration
SERVER_TYPE=http
SERVER_PORT=8080
SERVER_URL=https://evolution.yourdomain.com

# Cors
CORS_ORIGIN=*
CORS_METHODS=POST,GET,PUT,DELETE
CORS_CREDENTIALS=true

# Log
LOG_LEVEL=ERROR,WARN,DEBUG,INFO,LOG,VERBOSE,DARK,WEBHOOKS
LOG_COLOR=true
LOG_BAILEYS=error

# Instance
DEL_INSTANCE=false
DEL_TEMP_INSTANCES=true

# Webhook
WEBHOOK_GLOBAL_URL=
WEBHOOK_GLOBAL_ENABLED=false
WEBHOOK_GLOBAL_WEBHOOK_BY_EVENTS=false

# Rabbitmq
RABBITMQ_ENABLED=false

# SQS
SQS_ENABLED=false

# Websocket
WEBSOCKET_ENABLED=false

# Chatwoot
CHATWOOT_ENABLED=false

# Typebot
TYPEBOT_ENABLED=false

# Proxy
PROXY_HOST=
PROXY_PORT=
PROXY_PROTOCOL=
PROXY_USERNAME=
PROXY_PASSWORD=
